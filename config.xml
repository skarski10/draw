<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.helloworld" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>draw!</name>
    <description>
        A drawing prompt app.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Semic Group
    </author>
    <content src="index.html" />
    <access origin="*" />
    <access origin="http://staging.redleafhq.com/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <preference name="android-minSdkVersion" value="14" />
        <allow-intent href="market:*" />
        <splash density="land-hdpi" src="res/screen/android/splash-land-hdpi.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="BackupWebStorage" value="none" />
    </platform>
    <preference name="DisallowOverscroll" value="true" />
    <engine name="android" spec="^7.0.0" />
    <engine name="browser" spec="^5.0.3" />
    <plugin name="cordova-plugin-whitelist" spec="~1.2.0" />
    <plugin name="cordova-plugin-console" spec="~1.0.1" />
    <plugin name="cordova-sqlite-evcore-extbuild-free" spec="^0.9.3" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
    <plugin name="cordova-plugin-dbcopy" spec="git+https://github.com/an-rahulpandey/cordova-plugin-dbcopy.git" />
</widget>
